{
    "name": "RustPacker Dev Container",
     "image": "mcr.microsoft.com/devcontainers/rust:latest",
    "build": {
        "dockerfile": "Dockerfile"
    },
     "updateContentCommand": "rustup target add x86_64-pc-windows-gnu",
    "settings": {
        "terminal.integrated.shell.linux": "/bin/bash"
    },
    "customizations": {
         "vscode": {
             "extensions": [
                "rust-lang.rust"
             ]
         }
    },
    "postCreateCommand": "cargo build"
  }